pipeline {
    agent any

    stages {
        stage("SCM Checkout") {
            steps {
                git branch: 'master',
                    credentialsId: "github-access",                
                    url: "https://github.com/avbravikanth1/my-react-app.git"            
            }
        }
        stages {
          stage('Build') {
            steps {
                sh "npm install"
               
            }
        }
        }
}
