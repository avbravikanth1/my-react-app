pipeline {
    agent any

    stages {
        stage("SCM Checkout") {
            steps {
                git branch: 'master',
                    credentialsId: "github-access",                
                    url: "https://github.com/avbravikanth1/my-react-app.git"            
            }
        }
        
          stage('Build') {
            steps {
                sh "npm install"
                sh "run clean"
                sh"npm run build"
               
            }
        
        }
}
}