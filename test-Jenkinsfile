pipeline {
    agent any

    stages {
        stage("SCM Checkout") {
            steps {
                git branch: 'master',
                    credentialsId: "github-access",                
                    url: "https://github.com/avbravikanth1/my-react-app.git"            
            }
        }
        stage('Build') {
            steps {
                sh "npm run remove"
                sh "npm install"
                sh "npm run clean"
                sh "nmp run build"
                
            }
        }
        
}
